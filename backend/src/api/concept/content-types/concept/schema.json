{
  "kind": "collectionType",
  "collectionName": "concepts",
  "info": {
    "singularName": "concept",
    "pluralName": "concepts",
    "displayName": "Concept",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "concept_title": {
      "type": "string"
    },
    "concept_location": {
      "type": "string"
    },
    "surface": {
      "type": "integer"
    },
    "main_image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "main_description": {
      "type": "richtext"
    },
    "layouts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::layout.layout",
      "mappedBy": "concept"
    },
    "concept_intro": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::concept-description.concept-description"
    }
  }
}
