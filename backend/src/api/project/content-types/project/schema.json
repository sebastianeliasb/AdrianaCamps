    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "project_title": {
      "type": "string"
    },
    "project_location": {
      "type": "string"
    },
    "project_date": {
      "type": "date"
    },
    "client": {
      "type": "string"
    },
    "photographer": {
      "type": "string"
    },
    "surface": {
      "type": "integer"
    },
    "main_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
